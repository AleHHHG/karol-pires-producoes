  </div>
</div>
<% if production_sheet.transport_images.attached? %>
  <div class="fotorama" data-nav="thumbs">
    <% production_sheet.transport_images.each do |file| %>
          <%= image_tag file %>
    <% end %>
  </div>
  <br clear="all">
<% end %>