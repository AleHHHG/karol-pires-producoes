<div class="row mb-3">
  <div class="col-sm-4"></div>
  <div class="col-sm-4"><strong>Nome</strong></div>
  <div class="col-sm-4"><strong>Contato</strong></div>
</div>
 <div class="row mb-3">
  <%= form.label 'Produtor do evento', class: 'col-sm-4 col-form-label' %>
